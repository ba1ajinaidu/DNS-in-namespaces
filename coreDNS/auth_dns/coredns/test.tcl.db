;
; BIND data file for local loopback interface
$ORIGIN test.tcl.
$TTL	604800
@	IN	SOA	ns1.test.tcl. admin.test.tcl. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL

; name servers - NS records
test.tcl.     IN      NS      ns1.test.tcl.
ns1.test.tcl. IN    A   10.1.1.3

; A records
t1 IN A 10.0.0.3
t2 IN A 10.0.0.2
